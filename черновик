from aiogram import Bot, types
from aiogram.dispatcher import Dispatcher
from aiogram.utils import executor

API_TOKEN = 'YOUR_TELEGRAM_BOT_API_TOKEN'
bot = Bot(token=API_TOKEN)
dp = Dispatcher(bot)

@dp.message_handler(commands=['getaddress'])
async def send_address(message: types.Message):
    lat, lon = get_coordinates_from_address("Толе би 59")
    if lat and lon:
        await bot.send_location(chat_id=message.chat.id, latitude=lat, longitude=lon)
    else:
        await message.reply("Не удалось определить координаты для адреса.")

if __name__ == '__main__':
    executor.start_polling(dp, skip_updates=True)
